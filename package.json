{
  "name": "catalog",
  "version": "0.3.8",
  "description": "Open source audio software and hardware catalog",
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "validate": "tsx scripts/validate.ts",
    "build": "tsx scripts/build-sqlite.ts",
    "patch": "tsx scripts/generate-patch.ts",
    "dev": "tsx watch scripts/validate.ts",
    "changeset": "changeset",
    "version": "pnpm rebuild-slug-index && changeset version",
    "release": "changeset publish",
    "check-slugs": "tsx scripts/check-slugs.ts",
    "rebuild-slug-index": "tsx scripts/rebuild-slug-index.ts",
    "format": "prettier --write \"data/**/*.yaml\"",
    "format:check": "prettier --check \"data/**/*.yaml\"",
    "validate-urls": "tsx scripts/validate-urls.ts"
  },
  "keywords": [
    "audio",
    "plugins",
    "vst",
    "au",
    "daw",
    "music-production",
    "catalog"
  ],
  "author": "Jeffrey Louden <jeff@racks.fm>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@types/better-sqlite3": "7.6.13",
    "@types/node": "25.0.3",
    "better-sqlite3": "12.5.0",
    "marked": "^17.0.1",
    "prettier": "^3.7.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "yaml": "2.8.2",
    "zod": "4.3.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
